<%= form_for @item do |f|%>
	<%= f.label :name%>
	<%= f.text_field :name %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.label :like %>
	<%= f.button :like %>

	<%= f.label :complete %>
	<%= f.check_box :complete %>

	<%= f.label :type %>
	<%= f.select :type %>

	<%= f.submit %>

<% end %>