<%= form_for [@list, @item] do |f|%>
	<%= f.label :name%>
	<%= f.text_field :name %>

	<%= f.label :description %>
	<%= f.text_area :description %>

	<%= f.label :like %>
	<%= f.button :like %>

	<%= f.label :complete %>
	<%= f.check_box :complete %>

	<%= f.label :classification %>
	<%= f.select :classification, options_for_select(["Select One", "travel","sports","shows/plays","Miscellaneous"]) %>

	<%= f.submit %>

<% end %>